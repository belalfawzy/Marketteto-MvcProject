@model List<Category>

<div class="row">
    <div class="col-md-9 offset-md-2">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Description</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var v in Model)
                {
                    <tr>
                        <td>@v.Name </td>
                        <td>@v.Description</td>
                        <td><a class="btn btn-outline-info"><i class="fas fa-regular fa-pencil mr-2"></i>Edit</a></td>
                        <td><a class="btn btn-outline-secondary"><i class="fas fa-regular fa-eye mr-2"></i>Details</a></td>
                        <td><a class="btn btn-outline-danger"><i class="fas fa-regular fa-trash mr-2"></i>Delete</a></td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
